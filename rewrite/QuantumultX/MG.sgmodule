#!name=MG
#!desc=MG

[URL Rewrite]
#播放页开通提示移除
http://vip.bz.mgtv.com/client/dynamic_entry - reject

[Script]
MG1 = type=http-request, pattern=^http[s]?:\/\/mobile\.api\.mgtv\.com\/v[0-9]\/(playlist|video\/album|video\/relative|video\/list).*$, script-path=https://raw.githubusercontent.com/pxx917144686/ios/master/rewrite/QuantumultX/MG1.js, timeout=60

MG = type=http-request, pattern=https://mobile-stream.api.mgtv.com/v1/video/source?, script-path=https://raw.githubusercontent.com/pxx917144686/ios/master/rewrite/QuantumultX/MG.js, timeout=60

MG = type=http-response, pattern=https://nuc.api.mgtv.com/GetUserInfo, script-path=https://raw.githubusercontent.com/pxx917144686/ios/master/rewrite/QuantumultX/MG.js, requires-body=true, max-size=-1, timeout=60

MG = type=http-response, pattern=https://mobile-stream.api.mgtv.com/v1/video/source, script-path=https://raw.githubusercontent.com/pxx917144686/ios/master/rewrite/QuantumultX/MG.js, requires-body=true, max-size=-1, timeout=60

#港区
MG = type=http-request, pattern=^https://mobile.api.mgtv.com/v8/video/getSource, script-path=https://raw.githubusercontent.com/pxx917144686/ios/master/rewrite/QuantumultX/MG.js, timeout=60

MG = type=http-request, pattern=https://pad.api.mgtv.com/v8/video/getSource, script-path=https://raw.githubusercontent.com/pxx917144686/ios/master/rewrite/QuantumultX/MG.js, timeout=60
#mac
MG = type=http-request, pattern=https://pcc.api.mgtv.com/video/getSource, script-path=https://raw.githubusercontent.com/pxx917144686/ios/master/rewrite/QuantumultX/MG.js, timeout=60

[MITM]
hostname = %APPEND% *.mgtv.com, pad.api.mgtv.com, pcc.api.mgtv.com
